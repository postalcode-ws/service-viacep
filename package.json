{
  "name": "@postalcode/service-viacep",
  "version": "2.0.0",
  "description": "Plugin for postalcode",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "exports": "./dist/index.modern.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.min.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "npm run build:web && npm run build:node",
    "test": "mocha --config .mocharc.json",
    "test:watch": "npm run test --watch",
    "coverage": "nyc --reporter=lcov npm test",
    "build:web": "microbundle -f umd --external none",
    "build:node": "microbundle -f modern,es,cjs"
  },
  "browser": "dist/index-browser.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/postalcode-ws/service-viacep.git"
  },
  "author": "Welton Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/postalcode-ws/service-viacep/issues"
  },
  "keywords": [
    "cep",
    "zipcode",
    "zip",
    "promise",
    "viacep",
    "postalcode",
    "postcode",
    "brazil"
  ],
  "homepage": "https://postalcode.ws",
  "peerDependencies": {
    "@postalcode/postalcode": "1.0.3"
  },
  "devDependencies": {
    "@postalcode/postalcode": "^1.0.3",
    "@types/chai": "^4.2.15",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "chai": "^4.3.3",
    "chai-subset": "^1.6.0",
    "microbundle": "^0.13.0",
    "mocha": "^8.3.1",
    "nock": "13.0.2",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.2"
  },
  "files": [
    "dist",
    "index.d.ts"
  ]
}
